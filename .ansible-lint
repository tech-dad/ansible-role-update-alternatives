# .ansible-lint
exclude_paths:
  - /root/.cache/
  - .cache/  # implicit unless exclude_paths is defined in config
  - .git/

parseable: true

skip_list:
  - experimental # Don't warn us about experimental/WIP linting tasks
  - fqcn-builtins # made implicit with ansiblelint 6: https://github.com/ansible/ansible-lint/releases/tag/v6.0.0
